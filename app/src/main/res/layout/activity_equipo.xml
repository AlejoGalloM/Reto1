<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".Equipo">

    <TextView
        android:id="@+id/Alejo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Alejo1"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.437"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.451" />

    <TextView
        android:id="@+id/Alejo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Alejo2"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.923" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="242dp"
        android:layout_height="246dp"
        android:layout_marginStart="138dp"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="146dp"
        android:layout_marginBottom="56dp"
        app:layout_constraintBottom_toTopOf="@+id/Alejo1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/img_20200824_wa0010_01" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="248dp"
        android:layout_height="258dp"
        app:srcCompat="@drawable/img_20200822_000713_942"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="370dp" />

</androidx.constraintlayout.widget.ConstraintLayout>